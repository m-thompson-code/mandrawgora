{
  "name": "mandrawgora",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:dev",
    "start:dev": "npm run env:dev && ng serve --progress --open",
    "start:prod": "npm run env:prod && ng serve --progress --open",
    "build": "npm run build:dev",
    "build:dev": "npm run env:dev && ng build --prod",
    "build:prod": "npm run env:prod && ng build --prod",
    "host": "firebase deploy --only hosting",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "npm run build:dev && npm run host",
    "deploy:prod": "npm run build:prod && npm run host",
    "env:dev": "firebase use dev && node env-handler dev",
    "env:prod": "firebase use prod && node env-handler prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.1",
    "@angular/cdk": "^10.2.6",
    "@angular/common": "~10.2.1",
    "@angular/compiler": "~10.2.1",
    "@angular/core": "~10.2.1",
    "@angular/forms": "~10.2.1",
    "@angular/material": "^10.2.6",
    "@angular/platform-browser": "~10.2.1",
    "@angular/platform-browser-dynamic": "~10.2.1",
    "@angular/router": "~10.2.1",
    "cross-fetch": "^3.0.6",
    "firebase": "^8.0.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.29.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.1",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
